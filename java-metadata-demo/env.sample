# Neo4j connection settings
# Copy this file to .env and fill in your credentials

# JDBC URL for Neo4j connection
# Examples:
#   jdbc:neo4j://localhost:7687          (local instance)
#   jdbc:neo4j+s://xxx.databases.neo4j.io (Aura cloud instance)
NEO4J_URL=jdbc:neo4j://localhost:7687

# Database credentials
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-password-here
